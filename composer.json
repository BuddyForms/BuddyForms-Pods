{
  "name": "themekraft/buddyforms-pods",
  "description": "Use BuddyForms with Pods",
  "type": "wordpress-plugin",
  "keywords": [
    "frontend table",
    "buddyforms",
    "datatable"
  ],
  "homepage": "https://github.com/BuddyForms/buddyforms-frontend-table/",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Sven Lehnert",
      "email": "svenl@themekraft.com"
    },
    {
      "name": "Guillermo Figueroa Mesa",
      "email": "gfirem@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "consolidation/robo": "^1.0.0",
    "bamarni/composer-bin-plugin": "dev-master"
  },
  "scripts": {
    "bin": "echo 'bin not installed'",
    "post-install-cmd": [
      "@composer bin all install --ansi"
    ],
    "post-update-cmd": [
      "@composer bin all update --ansi"
    ]
  },
  "extra": {
    "bamarni-bin": {
      "target-directory": "vendor-scope"
    }
  }
}
